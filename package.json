{
  "name": "@balena/jellyfish-plugin-outreach",
  "description": "Outreach Jellyfish Plugin",
  "version": "1.0.398",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-outreach"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "npx jest --runInBand --bail test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-action-library": "^16.2.5",
    "@balena/jellyfish-assert": "^1.2.7",
    "@balena/jellyfish-environment": "^6.0.9",
    "@balena/jellyfish-plugin-base": "^2.2.25",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-core": "^11.1.6",
    "@balena/jellyfish-plugin-default": "^21.2.0",
    "@balena/jellyfish-sync": "^6.3.0",
    "@balena/jellyfish-test-harness": "^9.2.2",
    "@balena/jellyfish-types": "^2.0.0",
    "@balena/lint": "^6.2.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.4",
    "deplint": "^1.1.3",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "nock": "^13.2.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-01-17T18:00:34.912Z"
  }
}
